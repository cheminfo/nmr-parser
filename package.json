{
  "name": "nmr-parser",
  "version": "1.9.0",
  "description": "Read and convert any NMR file",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "compile": "rollup -c",
    "prepublishOnly": "npm run compile",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run compile && npm run test-coverage && npm run eslint && npm run prettier",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "build": "cheminfo-build --entry src/index.js --root NMRparser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmr-parser.git"
  },
  "keywords": [
    "nmr",
    "magnetic resonance",
    "parser",
    "bruker",
    "JEOL",
    "CSDM",
    "data analysis"
  ],
  "author": "Julien Wist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/nmr-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/nmr-parser#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@rollup/plugin-json": "^4.1.0",
    "@types/jest": "^27.5.0",
    "bruker-data-test": "^0.2.1",
    "cheminfo-build": "^1.1.11",
    "eslint": "^8.15.0",
    "eslint-config-cheminfo": "^8.0.2",
    "filelist-utils": "^0.5.0",
    "jcamp-data-test": "^0.0.5",
    "jeol-data-test": "^0.3.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.77.0"
  },
  "dependencies": {
    "brukerconverter": "^4.1.0",
    "is-any-array": "^2.0.0",
    "jcampconverter": "^9.0.1",
    "jeolconverter": "^1.0.1",
    "nmr-processing": "^8.3.0"
  }
}